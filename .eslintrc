{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"extends": ["eslint:recommended", "google"],
	"rules": {
		"semi": "off",
		"object-curly-spacing": "off",
		"linebreak-style": "off",
		"comma-dangle": "off",
		"new-cap": "off",
		"require-jsdoc": "off",
		"no-tabs": "off",
		"no-unused-vars": "off",
		"prefer-const": "off"
	}
}